<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="accountService" class="cn.gandan.service.impl.AccountServiceImpl">

        <property name="name" value="盛利"></property>

        <property name="strings">
            <list>
                <value>AAA</value>
                <value>BBB</value>
                <value>CCC</value>
            </list>
        </property>

        <property name="maps">
            <props>
                <prop key="CCC">ccc</prop>
                <prop key="DDD">ddd</prop>
                <prop key="EEE">eee</prop>
            </props>
        </property>

    </bean>

    <bean id="accountDao" class="cn.gandan.dao.impl.AccountDaoImpl"></bean>

</beans>